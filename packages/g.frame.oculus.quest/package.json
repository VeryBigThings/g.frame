{
  "name": "@g.frame/oculus.quest",
  "version": "0.2.2",
  "description": "OIMO physics module for g.frame framework",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:assets": "copyfiles -f -s ./src/assets/models/* ./build/main/assets/models/",
    "clean": "trash build test",
    "test": "jest --passWithNoTests",
    "lint": "tslint \"src/**/*.ts?(x)\"",
    "docs": "trash docs && typedoc && touch docs/.nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VeryBigThings/g.frame.git"
  },
  "keywords": [
    "webgl",
    "three.js",
    "g.frame"
  ],
  "author": "Very Big Things",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VeryBigThings/g.frame/issues"
  },
  "homepage": "https://github.com/VeryBigThings/g.frame#readme",
  "dependencies": {
    "@g.frame/common.action_controller": "^0.2.0",
    "@g.frame/common.loaders": "^0.2.0",
    "@g.frame/common.picking_controller": "^0.2.0",
    "@g.frame/common.xr_manager": "^0.2.1"
  },
  "peerDependencies": {
    "three": "^0.123.0",
    "@g.frame/core": "^0.2.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "copyfiles": "^2.2.0",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "trash-cli": "^3.0.0",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3",
    "three": "^0.123.0",
    "@g.frame/core": "^0.2.0"
  },
  "gitHead": "2adcda2e0de0598d1351dea6068d39f70edc7f99"
}
