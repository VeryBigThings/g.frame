  },
  "homepage": "https://github.com/VeryBigThings/g.frame#readme",
  "dependencies": {
    "@g.frame/common.action_controller": "^0.3.0"
  },
  "peerDependencies": {
    "@g.frame/core": "^0.3.0",
    "three": "0.144.0"
  },
  "devDependencies": {
    "@g.frame/core": "^0.3.0",
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.17",
    "@types/three": "0.144.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "three": "0.144.0",
    "trash-cli": "^5.0.0",
    "ts-jest": "^29.0.0",
    "tslint": "^6.1.0",
    "typedoc": "^0.23.14",
    "typescript": "^4.8.3"
  },
  "gitHead": "2adcda2e0de0598d1351dea6068d39f70edc7f99"
